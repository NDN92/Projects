package de.NDN.app.writeInvoice.document;

import java.util.ArrayList;

import de.NDN.app.globalObjects.CustomerType;

public class DocumentInvoice {
	
	private CustomerType customerType;
	private ArrayList<PageInvoice> pages;
	private PageInvoice pages[];
	
	public DocumentInvoice(CustomerType customerType, PageInvoice pages[]) {
		this.customerType = customerType;
		this.pages = pages;
	}

	public PageInvoice[] getPages() {
		return pages;
	}

	public void setPages(PageInvoice pages[]) {
		this.pages = pages;
	}
	
}
